<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" maxWidth="360.0" minWidth="240.0" prefHeight="600.0" prefWidth="270.0" style="-fx-background-color: #DDE3E5;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <!--fx:controller="controller.DrawerController">-->
    <children>
        <JFXButton fx:id="newmail_btn" accessibleText="new_mail" alignment="BOTTOM_LEFT" buttonType="RAISED" prefHeight="44.0" prefWidth="300.0" text="NEW MAIL" textFill="#54595e">
            <font>
                <Font name="BebasNeueBold" size="24.0" />
            </font>
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </JFXButton>
        <JFXButton accessibleText="sent" alignment="BOTTOM_LEFT" buttonType="RAISED" prefHeight="44.0" prefWidth="300.0" text="UNREAD" textFill="#54595e">
            <font>
                <Font name="BebasNeueBold" size="24.0" />
            </font>
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </JFXButton>
        <JFXButton fx:id="inbox_btn" accessibleText="inbox" alignment="BOTTOM_LEFT" buttonType="RAISED" prefHeight="44.0" prefWidth="300.0" text="INBOX" textFill="#54595e">
            <font>
                <Font name="BebasNeueBold" size="24.0" />
            </font>
            <VBox.margin>
                <Insets top="12.0" />
            </VBox.margin>
        </JFXButton>
        <JFXButton accessibleText="draft" alignment="BOTTOM_LEFT" buttonType="RAISED" prefHeight="44.0" prefWidth="300.0" text="OUTBOX" textFill="#54595e">
            <font>
                <Font name="BebasNeueBold" size="24.0" />
            </font>
            <VBox.margin>
                <Insets top="14.0" />
            </VBox.margin>
        </JFXButton>
    </children>
    <padding>
        <Insets left="40.0" />
    </padding>
</VBox>
